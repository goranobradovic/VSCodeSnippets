    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>ClassName</ID>
          <ToolTip>name of class</ToolTip>
          <Default>MyClass</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>SubClass</ID>
          <ToolTip>name of subclass</ToolTip>
          <Default>SubClass</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>jQuery</ID>
          <ToolTip>jQuery</ToolTip>
          <Default>$</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>MethodName</ID>
          <ToolTip>MethodName</ToolTip>
          <Default>MethodName</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="javascript"><![CDATA[
var $ClassName$ = (function ($ClassName$) {
	/// <summary>
	/// part of the $ClassName$ module
	/// </summary>
    var self = $ClassName$;

    function $SubClass$() {
	/// <summary>
	/// $SubClass$ submodule of the $ClassName$
	/// </summary>
        var _$MethodName$ = function(){
	        /// <summary>
	        /// $MethodName$ method in $ClassName$.$SubClass$
	        /// </summary>
        }
        return {
	        /// $ClassName$.$MethodName$.$MethodName$()
            $MethodName$: _$MethodName$
        };
    }

    function init() {
	    /// <summary>
        /// This places result of $SubClass$() execution under
	    /// $ClassName$.$MailConfigurator$
	    /// </summary>
        self.$SubClass$ = self.$SubClass$ || $SubClass$();
    };
    $jQuery$(document).ready(init);
    return self;
}($ClassName$ || {}));
]]></Code>
    </Snippet>
  </CodeSnippet>
